<!doctype html>
<html lang="es">
 <head> 
  <title>Clima</title> 
  <meta charset="UTF-8"> 
  <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
  <meta http-equiv="X-UA-Compatible" content="ie=edge"> 
   <%- include('partials/head'); %>		      
   <link rel="stylesheet" href="./stylesheets/styleclima.css">
 </head> 
 <body id="clima"> 
  <%- include('partials/navbar'); %>
  <div class="app-wrap"> 
   <header> 
    <input type="text" autocomplete="on" class="search-box" placeholder="Busca una ciudad..."> 
   </header> 
   <main> 
    <section class="location"> 
     <div class="city">
       San Miguel de Allende 
     </div> 
     <div class="date">
       Miércoles 22 Octubre 2020 
     </div> 
    </section> 
    <div class="current"> 
     <div class="temp">
       22 
      <span>°c</span> 
     </div> 
     <div class="weather">
       Soleado 
     </div> 
     <div class="hi-low">
       13°c / 16°c 
     </div> 
    </div> 
   </main> 
  </div> 
  <%- include('partials/footer'); %>
  <%- include('partials/script'); %>
 </body>
</html>
